  },
  "dependencies": {
    "reflect-metadata": "^0.2.2",
    "snowyflake": "^2.0.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "plop": "^4.0.1",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.4.5"
  }
}
