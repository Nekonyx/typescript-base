{
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "start": "node --enable-source-maps build/index.js",
    "watch": "tsc -p tsconfig.build.json -w",
    "build": "tsc -p tsconfig.build.json",
    "lint": "biome lint src",
    "format": "biome format src --write",
    "dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node --enable-source-maps build/index.js\""
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/node": "^20.16.3",
    "tsc-watch": "^7.0.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "typedi": "^0.10.0"
  }
}
