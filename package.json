{
  "private": true,
  "packageManager": "pnpm@9.0.6",
  "scripts": {
    "start": "node --enable-source-maps build/index.js",
    "watch": "tsc -w",
    "build": "tsc",
    "dev": "tsc-watch --onSuccess \"pnpm start\"",
